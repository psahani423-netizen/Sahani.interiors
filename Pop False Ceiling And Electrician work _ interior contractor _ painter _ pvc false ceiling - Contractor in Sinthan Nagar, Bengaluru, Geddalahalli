  .ads-hero-section .text-container.has-tagline { top: -16px; }
  .ads-hero-section .text-container .pitch-points-wrapper { margin-top: 24px; }
  .ads-hero-section .text-container .ctas-container { width: 100%; flex-direction: column; justify-content: start; gap: 8px; }
  .ads-hero-section .text-container .ctas-container .btn { width: 100%; justify-content: center; align-items: center; }
  .ads-hero-section .img-container { position: relative; left: unset; width: 100%; right: unset; height: unset; }
  .pitch-point .pitch-point-text { font-size: 16px; font-weight: 600; }
  .value-proposition-section, .value-proposition-section .container { padding: 0px; }
  .value-proposition-section .content-container { margin-left: 0px; padding: 32px 18px 0px; }
  .value-proposition-section .img-container { height: fit-content; width: 100%; }
  .value-proposition-section .value-props-wrapper { flex-flow: column wrap; gap: 16px; }
  .value-proposition-section .value-props-wrapper .value-prop-item { flex: 0 0 100%; }
